{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "src/**/*.js": {
      "runtime": "nodejs22.x"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
